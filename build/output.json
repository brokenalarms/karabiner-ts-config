  "rules": [
    {
      "description": "Layer - symbol-mode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "slash"
          },
          "to": [
            {
              "set_variable": {
                "name": "symbol-mode",
                "value": 1
              }
            },
            {
              "set_variable": {
                "name": "__layer",
                "value": 1
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "symbol-mode",
              "value": 1
            },
            {
              "type": "variable_unless",
              "name": "__layer",
              "value": 1
            }
          ],
          "to_if_alone": [
            {
              "key_code": "slash"
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "symbol-mode",
                "value": 0
              }
            },
            {
              "set_variable": {
                "name": "__layer",
                "value": 0
              }
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "1"
          },
          "to": [
            {
              "shell_command": "osascript -e '\nset prev to the clipboard\nset the clipboard to \"⇧\"\ntell application \"System Events\"\n  keystroke \"v\" using command down\n  delay 0.1\nend tell\nset the clipboard to prev'"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "symbol-mode",
              "value": 1
            }
          ]
        },
